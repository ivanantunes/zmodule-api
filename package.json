{
  "name": "zmodule-api",
  "version": "0.1.13",
  "description": "A library created with the concept of facilitating the creation of api.",
  "bin": "lib/index.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "postversion": "git push && git push --tags",
    "dist:patch": "npm version patch && npm run build && npm publish",
    "dist:minor": "npm version minor && npm run build && npm publish",
    "dist:major": "npm version major && npm run build && npm publish",
    "start": "ts-node src/test/index.ts",
    "watch": "nodemon -e ts -w ./src -x npm run watch:serve",
    "watch:serve": "ts-node src/test/index.ts",
    "build": "rm -rf lib/ && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivanantunes/zmodule-api.git"
  },
  "keywords": [
    "api",
    "module",
    "node",
    "js",
    "javascript",
    "rest",
    "typescript"
  ],
  "author": "Ivan Antunes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivanantunes/zmodule-api/issues"
  },
  "homepage": "https://github.com/ivanantunes/zmodule-api#readme",
  "dependencies": {
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "i18next": "^19.8.7",
    "mariadb": "^3.0.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.8",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "rxjs": "^6.6.3",
    "sequelize": "^6.23.0",
    "socket.io": "^4.5.2",
    "superagent": "^8.0.0",
    "tedious": "^15.1.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/faker": "^6.6.9",
    "@types/helmet": "^4.0.0",
    "@types/node": "^18.7.18",
    "@types/nodemailer": "^6.4.6",
    "@types/nodemailer-direct-transport": "^1.0.32",
    "@types/nodemailer-smtp-transport": "^2.7.5",
    "@types/sequelize": "^4.28.14",
    "@types/superagent": "^4.1.15",
    "faker": "^6.6.6",
    "jsdoc": "^3.6.11",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.8.3"
  }
}
