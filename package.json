{
  "name": "zmodule-api",
  "version": "0.1.2",
  "description": "A library created with the concept of facilitating the creation of api.",
  "bin": "lib/index.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "postversion": "git push && git push --tags",
    "dist:patch": "npm version patch && npm run build && npm publish",
    "dist:minor": "npm version minor && npm run build && npm publish",
    "dist:major": "npm version major && npm run build && npm publish",
    "start": "ts-node src/test/index.ts",
    "watch": "nodemon -e ts -w ./src -x npm run watch:serve",
    "watch:serve": "ts-node src/test/index.ts",
    "build": "rm -rf lib/ && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivanantunes/zmodule-api.git"
  },
  "keywords": [
    "api",
    "module",
    "node",
    "js",
    "javascript",
    "rest",
    "typescript"
  ],
  "author": "Ivan Antunes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivanantunes/zmodule-api/issues"
  },
  "homepage": "https://github.com/ivanantunes/zmodule-api#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "i18next": "^19.8.7",
    "mariadb": "^2.5.2",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.5.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "rxjs": "^6.6.3",
    "sequelize": "^6.5.0",
    "socket.io": "^4.0.1",
    "superagent": "^6.1.0",
    "tedious": "^11.0.3",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/crypto-js": "^4.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.6",
    "@types/helmet": "^4.0.0",
    "@types/node": "^14.14.33",
    "@types/nodemailer": "^6.4.0",
    "@types/nodemailer-direct-transport": "^1.0.31",
    "@types/nodemailer-smtp-transport": "^2.7.4",
    "@types/sequelize": "^4.28.9",
    "@types/superagent": "^4.1.10",
    "faker": "^5.4.0",
    "jsdoc": "^3.6.6",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  }
}
