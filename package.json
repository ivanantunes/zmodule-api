{
  "name": "zmodule-api",
  "version": "0.0.2",
  "description": "A library created with the concept of facilitating the creation of api.",
  "bin": "lib/index.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "postversion": "git push && git push --tags",
    "dist:patch": "npm version patch && npm publish",
    "dist:minor": "npm version minor && npm publish",
    "dist:major": "npm version major && npm publish",
    "start": "ts-node src/index.ts",
    "watch": "nodemon -e ts -w ./src -x npm run watch:serve",
    "watch:serve": "ts-node src/index.ts",
    "docs": "./node_modules/.bin/jsdoc -c docs.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivanantunes/zmodule-api.git"
  },
  "keywords": [
    "api",
    "module",
    "node",
    "js",
    "javascript",
    "rest",
    "typescript"
  ],
  "author": "Ivan Antunes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivanantunes/zmodule-api/issues"
  },
  "homepage": "https://github.com/ivanantunes/zmodule-api#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "mariadb": "^2.5.2",
    "mysql2": "^2.2.5",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "rxjs": "^6.6.3",
    "sequelize": "^6.5.0",
    "tedious": "^11.0.3",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^14.14.25",
    "@types/sequelize": "^4.28.9",
    "better-docs": "^2.3.2",
    "jsdoc": "^3.6.6",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  }
}
